{
  "manifest_version": 2,
  "version": "1",
  "name": "cooler git",
  "description": "replace tired git terms with wired replacements",
  "icons": {
    "48": "dealwithit.png"
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "cool git"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["run.js"]
    }
  ],
  "permissions": [
    "<all_urls>"
  ]
}
